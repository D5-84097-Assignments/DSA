package com.sunbeam;

public class NonRepeatingElement {

	public int firstNonRepeatingElement(int[] arr) {
		//int element = 0;
		boolean flag = false;
		for(int i = 0; i < arr.length-1; i++)
		{
			for(int j = i+1; j<arr.length-1; j++)
			{
				//element = arr[j];
				if(arr[i] == arr[j])
				{
					flag = true;
					break;
				}
			}
			if(flag == true) {
				break;
			}
			else
			{
				return arr[i];
			}
		}
	}

}
