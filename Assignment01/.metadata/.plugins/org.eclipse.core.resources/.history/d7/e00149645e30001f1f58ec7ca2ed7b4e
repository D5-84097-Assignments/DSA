package com.sunbeam;

public class BinarySearch {
	
	int search(int arr[],int key)
	{
		int start = arr.length -1;
		int end = 0;
		while(start <= end)
		{
			int mid = (start + end) / 2;
			if(arr[mid] == key)
			{
				return mid;
			}
			else if(arr[mid] < key)
			{
				end = mid -1;
			}
			else
			{
				start = mid + 1;
			}
		}
		return -1;
		
	}

}
